{
  "name": "comet-starter",
  "version": "1.0.0",
  "description": "React and TypeScript starter app, pre-configured with the Comet component library and other tools.",
  "license": "Apache-2.0",
  "main": "index.js",
  "keywords": [
    "uswds",
    "react",
    "components",
    "comet",
    "open source"
  ],
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode production --progress",
    "test": "jest --coverage",
    "lint": "eslint --ext .tsx --ext .ts src/ --fix --config .eslintrc.json",
    "format": "npx prettier src --write",
    "e2e": "npx cypress open",
    "e2e:ci": "rm reports/* || true && npx cypress run --reporter mochawesome",
    "e2e:report": "mochawesome-merge reports/*.json > reports/report.json && marge --reportDir reports reports/report.json"
  },
  "author": "",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "axe-core": "^4.7.2",
    "axios-mock-adapter": "^1.21.5",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "core-js": "^3.30.0",
    "css-loader": "^6.7.3",
    "cypress": "^12.14.0",
    "cypress-axe": "^1.4.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "node-sass": "^8.0.0",
    "prettier": "^2.8.7",
    "regenerator-runtime": "^0.13.11",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@metrostar/comet-data-viz": "1.1.1",
    "@metrostar/comet-extras": "^1.1.0",
    "@metrostar/comet-uswds": "1.3.1",
    "@uswds/uswds": "3.4.1",
    "axios": "^1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.10.0",
    "recoil": "^0.7.7"
  }
}
